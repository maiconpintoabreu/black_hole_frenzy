; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Back Hole Frenzy"
run/main_scene="res://level.tscn"
config/features=PackedStringArray("4.2", "Mobile")
run/max_fps=60
config/icon="res://Sprites/ship.png"

[autoload]

__InkRuntime="*res://addons/inkgd/ink_runtime.gd"
InkTemplate="*res://INK/INKTemplate.tscn"

[debug]

settings/stdout/print_fps=true

[display]

window/size/viewport_width=480
window/size/viewport_height=320
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/inkgd/plugin.cfg")

[inkgd]

register_templates=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

2d/run_on_separate_thread=true

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
environment/defaults/default_clear_color=Color(0.0705882, 0.0705882, 0.0705882, 1)
